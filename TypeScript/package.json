    "@types/lodash": "^4.14.116",
    "@types/mocha": "~2.2.41",
    "@types/node": "~7.0.18",
    "chai": "~3.5.0",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "rimraf": "~2.5.2",
    "stryker": "^0.29.1",
    "stryker-api": "^0.21.1",
    "stryker-mocha-framework": "^0.12.2",
    "stryker-mocha-runner": "^0.14.2",
    "stryker-typescript": "^0.13.2",
    "ts-node": "~3.1.0",
    "typescript": "^3.0.3"
  },
  "nyc": {
    "extension": [
      ".ts"
